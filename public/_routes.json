{
  "version": 1,
  "include": ["/*"],
  "exclude": [],
  "rules": [
    {
      "type": "header",
      "headers": {
        "Content-Type": "text/javascript; charset=utf-8"
      },
      "paths": [
        "/assets/*.js",
        "/assets/*.ts",
        "/assets/*.tsx",
        "/assets/*.mjs",
        "/*.js",
        "/*.ts",
        "/*.tsx",
        "/*.mjs"
      ]
    }
  ]
}
